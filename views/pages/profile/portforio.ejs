        <div class="header"><%=portfolioDataType1[i].pfoTitile%></div>
        <div class="meta">
          <span class="category"><%=portfolioDataType1[i].pfoCatagoryID%> | <i class="calendar alternate outline icon"></i><%=portfolioDataType1[i].pfoYears%></span>
        </div>
        <div class="description">
          <% if(portfolioDataType1[i].pfoIntro != '' && portfolioDataType1[i].pfoIntro != null && portfolioDataType1[i].pfoIntro != undefined){%>
          <div class="description">
            <p><%=portfolioDataType1[i].pfoIntro%></p>
          </div>
          <%}else{ %>

          <% } %>
        </div>
      </div>
    </a>
    <% }
