<div class="ui  secondary menu " style="font-family: 'Prompt', sans-serif;font-weight:200">
  <a class="active item" data-tab="aa">โครงการ</a>
  <a class="item" data-tab="ab">บริการวิชาการ</a>
  <a class="item" data-tab="ac">รางวัล</a>
  <a class="item" data-tab="ad">อื่น ๆ</a>
</div>


<div class="ui active tab basic segment" data-tab="aa">

  <% if(portfolioDataType1.length){ %>
  <div id="tabPFOType1">
    <div class="ui horizontal teal statistic tiny">
      <div class="label">กำลังแสดง</div>
      <div class="value " id="PFOType1Showonlabel"><%=portfolioDataType1.length%></div>
      <input type="hidden" id="PFOType1Showon" value="<%=portfolioDataType1.length%>" />
      <div class="label">จาก</div>
      <div class="value"><%=countAllPFOType1.countPFOType1%></div>
      <input type="hidden" id="countAllPFOType1input" value="<%=countAllPFOType1.countPFOType1%>" />
    </div>

    <% for( i = 0;i < portfolioDataType1.length;i++) {
       if(portfolioDataType1[i].pfoCatagoryID == 'โครงการ'){ %>
    <a class="ui link card fluid" href="/profile/<%=userData.id%>/<%=portfolioDataType1[i].pfoID%>">
      <div class="content">
        <div class="header"><%=portfolioDataType1[i].pfoTitile%></div>
        <div class="meta">
          <span class="category"><%=portfolioDataType1[i].pfoCatagoryID%> | <i class="calendar alternate outline icon"></i><%=portfolioDataType1[i].pfoYears%></span>
        </div>
        <div class="description">
          <% if(portfolioDataType1[i].pfoIntro != '' && portfolioDataType1[i].pfoIntro != null && portfolioDataType1[i].pfoIntro != undefined){%>
          <div class="description">
            <p><%=portfolioDataType1[i].pfoIntro%></p>
          </div>
          <%}else{ %>

          <% } %>
        </div>
      </div>
    </a>
    <% }
  }%>
  </div>
  <%if(portfolioDataType1.length >= countAllPFOType1.countPFOType1  ){ %>

  <%   }else{ %>
  <button class="fluid ui button" id="loadmorePFOType1" style="margin-top:1em">Load more</button>
  <% } %>
  <%   }else{ %>
  ยังไม่มีข้อมูลในส่วนนี้
  <% } %>

</div>



